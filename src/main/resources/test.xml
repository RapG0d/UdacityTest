<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="All">
    <test name="SignUp">
        <classes>
            <class name="TestUdacity">
                <methods>
                    <exclude name="checkInfo"/>
                </methods>
            </class>
            <class name="CourseTest">
                <methods>
                    <exclude name="checkCourseName"/>
                </methods>
            </class>
            <class name="NavigationTest">
                <methods>
                    <exclude name="checkCourseUrl"/>
                    <exclude name="checkUrlVRFoundation"/>
                    <exclude name="checkUrlVRMobile"/>
                </methods>
            </class>
            <class name="EditProfileTest">
                <methods>
                    <exclude name="editProfile"/>
                </methods>
            </class>
            <class name="SchoolTest">
                <methods>
                    <include name="checkSchoolName"/>
                </methods>
            </class>
        </classes>
    </test>
</suite>