<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="All">
    <test name="SignUp">
        <classes>
            <class name="TestUdacity">
                <methods>
                    <include name="checkInfo"/>
                </methods>
            </class>
            <class name="CourseTest">
                <methods>
                    <include name="checkCourseName"/>
                </methods>
            </class>
            <class name="NavigationTest">
                <methods>
                    <include name="checkCourseUrl"/>
                    <include name="checkUrlVRFoundation"/>
                    <include name="checkUrlVRMobile"/>
                </methods>
            </class>
        </classes>
    </test>
</suite>